<div class="container">
  <div class="row col-xs-10">
    <h2>Add course dates</h2>
  <div class="col-xs-5">
    <%= form_for [@course, @date] do |f| %>
    <%= f.date_select :date %>
  </div>
  <%#<div class="col-xs-5">%>
    <%= f.time_select :time %>
    <%= f.submit "add", class: "btn btn-default btn-xs" %>
    <% end %>
  <%#</div>%>
</div>
</div>

<%= render 'dates', dates: @dates, paid_dates: @paid_bookings %>
